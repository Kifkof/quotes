<MudFileUpload T="IBrowserFile" FilesChanged="Import">
    <ButtonTemplate>
        <MudButton HtmlTag="label"
                   Variant="Variant.Filled"
                   Color="Color.Primary"
                   StartIcon="@Icons.Filled.Image"
                   for="@context">
            Import File
        </MudButton>
    </ButtonTemplate>
</MudFileUpload>
<MudButton Color="Color.Warning" OnClick="@ExportAsync">Export</MudButton>
